<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <% images.forEach(image => { %>
    <div>
      <h3>Original Image</h3>
      <img src="<%= image.uploaded %>" alt="Original Image" />
      <h3>Resized Image</h3>
      <img src="<%= image.processed %>" alt="Resized Image" />
    </div>
    <% }); %>
  </body>
</html>
